# API Configuration
REACT_APP_API_URL=http://127.0.0.1:8000/api
REACT_APP_WEBSOCKET_URL=ws://127.0.0.1:8000/ws

# Authentication
REACT_APP_JWT_SECRET=replace_with_your_jwt_secret
REACT_APP_TOKEN_EXPIRY=8h

# External Services
REACT_APP_GEMINI_API_KEY=replace_with_your_gemini_api_key
REACT_APP_STRIPE_PUBLIC_KEY=replace_with_your_stripe_key

# Feature Flags
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_3D_PREVIEW=true
REACT_APP_ENABLE_ANALYTICS=false

# App Configuration
REACT_APP_NAME=LuxuryStay Hotel
REACT_APP_DESCRIPTION="Experience luxury and comfort at its finest"
REACT_APP_VERSION=1.0.0
REACT_APP_SUPPORT_EMAIL=support@luxurystay.com
REACT_APP_SUPPORT_PHONE=+1-123-456-7890

# Social Media Links
REACT_APP_FACEBOOK_URL=https://facebook.com/luxurystay
REACT_APP_TWITTER_URL=https://twitter.com/luxurystay
REACT_APP_INSTAGRAM_URL=https://instagram.com/luxurystay
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/luxurystay

# SEO Configuration
REACT_APP_META_TITLE="LuxuryStay Hotel - Luxury Accommodation"
REACT_APP_META_DESCRIPTION="Discover exceptional hospitality at LuxuryStay Hotel. Book your luxury stay today."
REACT_APP_META_KEYWORDS="luxury hotel, accommodation, luxury stay, hotel booking"

# Performance
REACT_APP_IMAGE_CDN_URL=https://cdn.example.com
REACT_APP_MAX_IMAGE_SIZE=5242880
REACT_APP_ENABLE_COMPRESSION=true

# Development
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_MOCK_API=false
REACT_APP_API_TIMEOUT=30000

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_RATE_LIMITING=true
REACT_APP_MAX_LOGIN_ATTEMPTS=5
REACT_APP_PASSWORD_MIN_LENGTH=8

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_DATE_FORMAT=MM/DD/YYYY

# Notifications
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false
REACT_APP_NOTIFICATION_DURATION=5000

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
REACT_APP_HOTJAR_ID=0000000
REACT_APP_ENABLE_ERROR_REPORTING=true

# Cache Configuration
REACT_APP_CACHE_DURATION=3600
REACT_APP_ENABLE_SERVICE_WORKER=true

# Map Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=replace_with_your_google_maps_api_key
REACT_APP_DEFAULT_LAT=40.7128
REACT_APP_DEFAULT_LNG=-74.0060
REACT_APP_DEFAULT_ZOOM=12

# Booking Configuration
REACT_APP_MIN_BOOKING_DAYS=1
REACT_APP_MAX_BOOKING_DAYS=30
REACT_APP_CANCELLATION_PERIOD=24
REACT_APP_CHECK_IN_TIME=15:00
REACT_APP_CHECK_OUT_TIME=11:00

# Important: Replace all sensitive values with your actual configuration
# Do not commit the actual .env file to version control
