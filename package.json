    "typescript": "^4.9.5",
    "whatwg-fetch": "^3.6.20"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,md,json}": [
      "prettier --write"
    ]
